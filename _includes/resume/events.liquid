<ul class="card-text font-weight-light list-group list-group-flush">
  {% assign events = data[1] | sort: 'year' | reverse %}
  {% for content in events %}
      <div class="row">
        <ul>
          <li>
            <h6 class="title ml-1 ml-md-4">
              {{ content.role }} <b class="font-weight-bold">@<a href="{{ content.url }}">{{ content.event }} {{ content.year}}</a></b>.
            </h6>
          </li>
        </ul>
    </div>
  {% endfor %}
</ul>
